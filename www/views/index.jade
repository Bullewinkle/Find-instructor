doctype html
html(lang="en")
	head
		title #{title}
		meta(http-equiv="Content-Type", content="text/html; charset=utf-8")
		script(src="js/spin.min.js")
		script(src="js/jquery-1.8.3.js", type="text/javascript")
		//- script(src="http://api-maps.yandex.ru/2.0/?load=package.full&lang=ru-RU&mode=debug", type="text/javascript")
		script(src="http://api-maps.yandex.ru/2.0/?load=package.full&lang=ru-RU", type="text/javascript")
		link(rel="stylesheet", href="css/normalize.css")
		link(rel="stylesheet", href="css/style.css")
		link(href="data:image/x-icon;base64,", rel="icon", type="image/x-icon")
		script(src="js/underscore-min.js")
		script(src="js/backbone-min.js")
		script(src="js/imagesloaded.min.js")
		script(src="js/dropzone.js")
		script(src="js/initmap.js")
		script 
			| var isAuthorised = '#{isAuthorised}';

	body(class= isAuthorised)
		header.header_main

			nav.main-menu.cf
				ul.main-menu-ul
					li.item: a.link(href="/") Главная
					li.item: a.link(href="/") Инструктора
					li.item: a.link(href="/") Клиенты
					li.item: a.link(href="/") Курорты
					li.item: a.link(href="/") Контакты
			.register
				ul
					li.signin.item
						a.show_popap.link(href="/signin") Вход
						.signin.popap
							.container
								form.signin.form(method="POST", action="/signin")
									label(for="email") E-mail:
									input(type="email", name="email", autocomplite="", value="developer085@gmail.com")
									label(for="pass") Пароль:
									input(type="password", name="pass", autocomplite="", value="085321")
									input.force_submit(type="submit", value="Войти")
								.req_res
					li.login.item
						a.show_popap.link(href="/login") Регистрация
						.login.popap
							.container
								form.login.form(method="POST", action="/login")
									label(for="name") Имя:
									input(type="text", name="name", autocomplite="", value="Дмитрий")
									br
									label(for="pass") Пароль:
									input(type="password", name="pass", autocomplite="", value="085321")
									br
									label(for="email") E-mail:
									input(type="email", name="email", autocomplite="", value="developer085@gmail.com")
									br
									label(for="pass_check") Еще разок пароль:
									input(type="password", name="pass_check", autocomplite="", value="085321")
									br
									label(for="type") Тип:
									input(type="radio", name="type", autocomplite="", value="инструктор", checked="")
									| Инструктор
									input(type="radio", name="type", autocomplite="", value="ученик")
									| Ученик
									br
									input.force_submit(type="submit", value="Зарегистрироваться")
								.req_res
					li.mypage.item
						a.show_userpage.link(href="/mypage") Моя страница
					li.signout.item
						a.show_popap.link(href="/signout") Выйти
		.content_main.cf
			.wrapper_left
				.user_page.cf
					.user_page_left
						.user_photo_container
							img.user_photo(src="/uploads/defaults/10.png", alt="")
					.user_page_right
						.user_name
						.user_info_container
							form#userInfo(method="POST", action="/user-update")
								ul.user_info_list
									li.user_info_item.cf
										label.user_info_label(for="email") E-mail: 
										input.user_info_content.user_email(type="text",name="email",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="phone") Телефон: 
										input.user_info_content.user_phone(type="text",name="phone",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="type") Тип: 
										input.user_info_content.user_type(type="text",name="type",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="age") Возраст: 
										input.user_info_content.user_age(type="text",name="age",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="chars") Личные качества: 
										input.user_info_content.user_chars(type="text",name="chars",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="main-phrase") Главное в горнолыжном спорте: 
										input.user_info_content.user_main_phrase(type="text",name="main-phrase",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="about-myself") О себе: 
										input.user_info_content.user_about_myself(type="text",name="about-myself",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="my-idol") Кумир горнолыжного спорта: 
										input.user_info_content.user_idol(type="text",name="my-idol",disabled="disabled", placeholder="-", value="")
									li.user_info_item.cf
										label.user_info_label(for="achieves") Достижения: 
										textarea.user_info_content.user_achievs(name="achieves", disabled="disabled", placeholder="-")
						button.edit_button.edit_my_page Редактировать

					.calendar
					.bottom_space
				button.close Закрыть
			#map
			.wrapper_right
				.my_page.cf
					.left
					.right
		footer.footer_main Copyright 2013 © 
	script(src="js/ready.js")
